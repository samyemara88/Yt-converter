<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#252a30"
    tools:context="com.example.selim_000.ytconvert.MainActivity">


    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="8dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="15dp"
            tools:layout_editor_absoluteY="154dp">


            <ImageView
                android:id="@+id/imageView"
                android:layout_width="105dp"
                android:layout_height="75dp"
                app:srcCompat="@mipmap/ic_launcher"
                tools:layout_editor_absoluteX="140dp"
                android:layout_marginTop="30dp"
                android:layout_marginBottom="30dp"
                tools:layout_editor_absoluteY="16dp" />

            <EditText
                android:id="@+id/editText"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginLeft="20dp"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Johnny Haliday - Le feu"
                android:textColor="@android:color/background_light"
                tools:layout_editor_absoluteX="15dp"
                tools:layout_editor_absoluteY="111dp" />


        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="15dp"
            tools:layout_editor_absoluteY="154dp"
            android:weightSum="1">


            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="105dp"
                android:layout_height="75dp"
                app:srcCompat="@mipmap/ic_launcher"
                tools:layout_editor_absoluteX="140dp"
                android:layout_marginTop="30dp"
                android:layout_marginBottom="30dp"
                tools:layout_editor_absoluteY="16dp" />

            <EditText
                android:id="@+id/editText2"
                android:layout_width="180dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginLeft="20dp"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Micakel Jackson - Beat it "
                android:textColor="@android:color/background_light"
                tools:layout_editor_absoluteX="15dp"
                tools:layout_editor_absoluteY="111dp"
                android:layout_weight="0.54" />


        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="15dp"
            tools:layout_editor_absoluteY="154dp"
            android:weightSum="1">


            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="105dp"
                android:layout_height="75dp"
                app:srcCompat="@mipmap/ic_launcher"
                tools:layout_editor_absoluteX="140dp"
                android:layout_marginTop="30dp"
                android:layout_marginBottom="30dp"
                tools:layout_editor_absoluteY="16dp" />

            <EditText
                android:id="@+id/editText3"
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginLeft="20dp"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Ariana Grande - side"
                android:textColor="@android:color/background_light"
                tools:layout_editor_absoluteX="15dp"
                tools:layout_editor_absoluteY="111dp" />


        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="15dp"
            tools:layout_editor_absoluteY="154dp">


            <ImageView
                android:id="@+id/rocky"
                android:layout_width="105dp"
                android:layout_height="75dp"
                app:srcCompat="@mipmap/ic_launcher"
                tools:layout_editor_absoluteX="140dp"
                android:layout_marginTop="30dp"
                android:layout_marginBottom="30dp"
                tools:layout_editor_absoluteY="16dp" />

            <EditText
                android:id="@+id/editText4"
                android:layout_width="180dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginLeft="20dp"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Cheb Khaled"
                android:textColor="@android:color/background_light"
                tools:layout_editor_absoluteX="15dp"
                tools:layout_editor_absoluteY="111dp" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="15dp"
            tools:layout_editor_absoluteY="154dp">


            <ImageView
                android:id="@+id/imageView5"
                android:layout_width="105dp"
                android:layout_height="75dp"
                app:srcCompat="@mipmap/ic_launcher"
                tools:layout_editor_absoluteX="140dp"
                android:layout_marginTop="30dp"
                android:layout_marginBottom="30dp"
                tools:layout_editor_absoluteY="16dp" />

            <EditText
                android:id="@+id/editText5"
                android:layout_width="180dp"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_marginLeft="20dp"
                android:ems="10"
                android:inputType="textPersonName"
                android:text="Dj Scooter"
                android:textColor="@android:color/background_light"
                tools:layout_editor_absoluteX="15dp"
                tools:layout_editor_absoluteY="111dp" />


        </LinearLayout>



    </LinearLayout>
</android.support.constraint.ConstraintLayout>